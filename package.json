{
	"name": "@spin-the-hexagon/tk",
	"module": "core/index.ts",
	"type": "module",
	"private": true,
	"version": "0.3.0",
	"devDependencies": {
		"@changesets/cli": "^2.29.8",
		"@types/bun": "latest",
		"oxfmt": "^0.17.0",
		"prettier-plugin-organize-imports": "^4.3.0"
	},
	"peerDependencies": {
		"typescript": "^5"
	},
	"scripts": {
		"start": "bun run ./core/index.ts",
		"fmt": "oxfmt . && stylua core/ plugins/ studio/ && cd native && cargo fmt && cd ../",
		"change": "bun run --bun changeset",
		"version": "bun run --bun changeset version",
		"make-natives-linux": "bun build-rust-natives && bun build --compile --target=bun-linux-x64-modern ./core/index.ts --outfile ./release/tk && zip ./release/tk-linux-x86_64.zip ./release/tk -m",
		"make-natives-macos": "bun build-rust-natives && bun build --compile --target=bun-darwin-arm64-modern ./core/index.ts --outfile ./release/tk && zip ./release/tk-macos-aarch64.zip ./release/tk -m",
		"make-natives-windows": "bun build-rust-natives && bun build --compile --target=bun-windows-x64-modern ./core/index.ts --outfile ./release/tk && zip ./release/tk-windows-x86_64.zip ./release/tk.exe -m",
		"make-natives": "bun make-natives-windows",
		"build-rust-natives": "cd native && cargo build",
		"dev": "bun run --watch ./core/index.ts dev ./example --no-cache",
		"dev-ts": "bun run --watch ./core/index.ts dev ./example-ts --no-cache",
		"plugin:dev": "bun run --watch --inspect ./core/index.ts dev ./studio --no-cache"
	},
	"dependencies": {
		"@johnnymorganz/stylua-bin": "^2.3.1",
		"@oxc-project/types": "^0.99.0",
		"@pppp606/ink-chart": "^0.2.4",
		"@rgrove/parse-xml": "^4.2.0",
		"@types/adm-zip": "^0.5.7",
		"@types/figlet": "^1.7.0",
		"@types/react": "^19.2.13",
		"@types/three": "^0.182.0",
		"chalk": "^5.6.2",
		"chokidar": "^4.0.3",
		"cmd-ts": "^0.14.3",
		"date-fns": "^4.1.0",
		"ebnf": "^1.9.1",
		"extract-zip": "^2.0.1",
		"figlet": "^1.10.0",
		"hono": "^4.10.7",
		"ink": "^6.6.0",
		"ink-link": "^5.0.0",
		"ink-spinner": "^5.0.0",
		"jotai": "^2.17.1",
		"oxc-parser": "^0.99.0",
		"oxc-resolver": "^11.15.0",
		"react": "^19.2.4",
		"smol-toml": "^1.4.2",
		"three": "^0.182.0",
		"ts-pattern": "^5.9.0",
		"valibot": "^1.1.0",
		"zimmerframe": "^1.1.4"
	},
	"trustedDependencies": [
		"@johnnymorganz/stylua-bin"
	]
}
