{
	"name": "@spin-the-hexagon/tk",
	"module": "src/index.ts",
	"type": "module",
	"private": true,
	"version": "0.0.5",
	"devDependencies": {
		"@changesets/cli": "^2.29.8",
		"@types/bun": "latest",
		"oxfmt": "^0.17.0",
		"prettier-plugin-organize-imports": "^4.3.0"
	},
	"peerDependencies": {
		"typescript": "^5"
	},
	"scripts": {
		"start": "bun run ./src/index.ts",
		"fmt": "oxfmt . && stylua src/ studio/",
		"change": "bun run --bun changeset",
		"version": "bun run --bun changeset version",
		"make-natives-linux": "bun build --compile --target=bun-linux-x64-modern ./src/index.ts --outfile ./release/tk && zip ./release/tk-linux-x86_64.zip ./release/tk -m",
		"make-natives-macos": "bun build --compile --target=bun-darwin-arm64-modern ./src/index.ts --outfile ./release/tk && zip ./release/tk-macos-aarch64.zip ./release/tk -m",
		"make-natives-windows": "bun build --compile --target=bun-windows-x64-modern ./src/index.ts --outfile ./release/tk && zip ./release/tk-windows-x86_64.zip ./release/tk.exe -m",
		"make-natives": "bun make-natives-linux && bun make-natives-windows && bun make-natives-macos",
		"dev": "bun run --watch ./src/index.ts dev ./example --no-cache",
		"dev-ts": "bun run --watch ./src/index.ts dev ./example-ts --no-cache",
		"plugin:dev": "bun run --watch --inspect ./src/index.ts dev ./studio --no-cache"
	},
	"dependencies": {
		"@johnnymorganz/stylua-bin": "^2.3.1",
		"@oxc-project/types": "^0.99.0",
		"@types/adm-zip": "^0.5.7",
		"chalk": "^5.6.2",
		"chokidar": "^4.0.3",
		"cmd-ts": "^0.14.3",
		"ebnf": "^1.9.1",
		"extract-zip": "^2.0.1",
		"figlet": "^1.9.4",
		"hono": "^4.10.7",
		"oxc-parser": "^0.99.0",
		"oxc-resolver": "^11.15.0",
		"smol-toml": "^1.4.2",
		"ts-pattern": "^5.9.0",
		"valibot": "^1.1.0",
		"zimmerframe": "^1.1.4"
	},
	"trustedDependencies": [
		"@johnnymorganz/stylua-bin"
	],
	"packageManager": "yarn@1.22.22+sha512.a6b2f7906b721bba3d67d4aff083df04dad64c399707841b7acf00f6b133b7ac24255f2652fa22ae3534329dc6180534e98d17432037ff6fd140556e2bb3137e"
}
