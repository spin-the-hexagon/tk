{
	"name": "@spin-the-hexagon/tk",
	"module": "src/index.ts",
	"type": "module",
	"private": true,
	"version": "0.0.5",
	"devDependencies": {
		"@types/bun": "latest",
		"prettier": "^3.6.2",
		"prettier-plugin-organize-imports": "^4.3.0",
		"@changesets/cli": "^2.29.8"
	},
	"peerDependencies": {
		"typescript": "^5"
	},
	"scripts": {
		"start": "bun run ./src/index.ts",
		"fmt": "prettier . -w",
		"change": "bun run --bun changeset",
		"version": "bun run --bun changeset version",
		"make-natives-linux": "bun build --compile --target=bun-linux-x64-modern ./src/index.ts --outfile ./release/tk && zip ./release/tk-linux-x86_64.zip ./release/tk -m",
		"make-natives-macos": "bun build --compile --target=bun-darwin-arm64-modern ./src/index.ts --outfile ./release/tk && zip ./release/tk-macos-aarch64.zip ./release/tk -m",
		"make-natives-windows": "bun build --compile --target=bun-windows-x64-modern ./src/index.ts --outfile ./release/tk && zip ./release/tk-windows-x86_64.zip ./release/tk.exe -m",
		"make-natives": "bun make-natives-linux && bun make-natives-windows && bun make-natives-macos",
		"dev": "bun run --watch ./src/index.ts dev ./example"
	},
	"dependencies": {
		"@types/adm-zip": "^0.5.7",
		"adm-zip": "^0.5.16",
		"chalk": "^5.6.2",
		"chokidar": "^4.0.3",
		"cmd-ts": "^0.14.3",
		"ebnf": "^1.9.1",
		"smol-toml": "^1.4.2",
		"valibot": "^1.1.0",
		"zimmerframe": "^1.1.4"
	}
}
